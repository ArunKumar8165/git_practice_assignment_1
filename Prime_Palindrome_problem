// palindrome problem.
let str= "nayan";
let bag="";
for (let i=1; i<=str.length-1; i++){
  if (str[i]==str[str.length-1]{
      bag="Yes";
      else {
      bag="No";
   }
   }
    console.log(bag)
   }

// primeproblem.
 let num= 13;
  let  count= 0;
 for (let i=1; i<=num; i++){
     if (num%i==0){
      count++;
   }
  }  
  if (count==2){
   console.log("Yes");
 } else   { 
   console.log("No");